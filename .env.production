# Production Environment Variables for Vercel
# Override these in Vercel dashboard with real API keys

# Required for E2B Sandboxing
E2B_API_KEY=placeholder_e2b_key

# Required for Web Scraping
FIRECRAWL_API_KEY=placeholder_firecrawl_key

# AI Providers (at least one required)
AVALAI_API_KEY=placeholder_avalai_key
AVALAI_BASE_URL=https://api.avalai.ir/v1
GOOGLE_GENERATIVE_AI_API_KEY=placeholder_google_key

# Optional AI Providers
ANTHROPIC_API_KEY=placeholder_anthropic_key
OPENAI_API_KEY=placeholder_openai_key
GROQ_API_KEY=placeholder_groq_key

# App Configuration
NODE_ENV=production
SKIP_ENV_VALIDATION=true